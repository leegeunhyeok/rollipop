{
  "name": "rollipop",
  "version": "0.1.0-alpha.0",
  "type": "module",
  "bin": "./bin/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "bin",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leegeunhyeok/rollipop.git",
    "directory": "packages/rollipop"
  },
  "author": "leegeunhyeok <dev.ghlee@gmail.com> (https://github.com/leegeunhyeok)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leegeunhyeok/rollipop/issues"
  },
  "homepage": "https://github.com/leegeunhyeok/rollipop#readme",
  "scripts": {
    "prepack": "yarn build",
    "typecheck": "tsc --noEmit",
    "build": "tsdown"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^14.0.0",
    "@inquirer/prompts": "^8.1.0",
    "@rollipop/common": "workspace:*",
    "@rollipop/core": "workspace:*",
    "@rollipop/dev-server": "workspace:*",
    "commander": "^14.0.2",
    "es-toolkit": "catalog:"
  },
  "devDependencies": {
    "tsdown": "catalog:",
    "typescript": "catalog:"
  }
}
