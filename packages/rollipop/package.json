{
  "name": "rollipop",
  "version": "0.1.0-alpha.7",
  "type": "module",
  "bin": "./bin/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./commands": {
      "import": {
        "types": "./dist/commands.d.ts",
        "default": "./dist/commands.js"
      },
      "require": {
        "types": "./dist/commands.d.cts",
        "default": "./dist/commands.cjs"
      }
    },
    "./pluginutils": {
      "import": {
        "types": "./dist/pluginutils.d.ts",
        "default": "./dist/pluginutils.js"
      },
      "require": {
        "types": "./dist/pluginutils.d.cts",
        "default": "./dist/pluginutils.cjs"
      }
    },
    "./runtime": {
      "import": {
        "types": "./dist/runtime.d.ts",
        "default": "./dist/runtime.js"
      },
      "require": {
        "types": "./dist/runtime.d.cts",
        "default": "./dist/runtime.cjs"
      }
    },
    "./hmr-runtime": "./dist/hmr-runtime.js",
    "./hmr-client": "./dist/hmr-client.js",
    "./client": {
      "types": "./client.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "client.d.ts",
    "bin",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leegeunhyeok/rollipop.git",
    "directory": "packages/rollipop"
  },
  "author": "leegeunhyeok <dev.ghlee@gmail.com> (https://github.com/leegeunhyeok)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leegeunhyeok/rollipop/issues"
  },
  "homepage": "https://github.com/leegeunhyeok/rollipop#readme",
  "scripts": {
    "prepack": "yarn build",
    "typecheck": "tsc --noEmit",
    "test": "vitest --run",
    "execute": "node bin/index.js",
    "build": "tsdown"
  },
  "dependencies": {
    "@babel/core": "^7.28.5",
