    "flow-remove-types": "^2.295.0",
    "gradient-string": "^3.0.0",
    "hermes-parser": "^0.33.0",
    "image-size": "^2.0.2",
    "json-schema-to-ts": "^3.1.1",
    "mime": "^4.1.0",
    "p-limit": "^7.2.0",
    "source-map": "^0.7.6",
    "strip-ansi": "^7.1.2",
    "url": "^0.11.4",
    "wrap-ansi": "^9.0.2",
    "ws": "^8.18.3"
  },
  "devDependencies": {
