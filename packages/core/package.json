        "types": "./dist/runtime.d.cts",
        "default": "./dist/runtime.cjs"
      }
    },
    "./hmr-runtime": "./dist/hmr-runtime.js",
    "./hmr-client": "./dist/hmr-client.cjs",
    "./client": {
      "types": "./client.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "client.d.ts",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leegeunhyeok/rollipop.git",
